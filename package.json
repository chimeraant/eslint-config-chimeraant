{
  "name": "eslint-config-chimeraant",
  "version": "0.0.0-semantic-release",
  "license": "MIT",
  "repository": "https://github.com/chimeraant/eslint-config-chimeraant.git",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "lint": "eslint --max-warnings=0 . --ignore-path .gitignore",
    "lint:fix": "pnpm lint --fix"
  },
  "eslintConfig": {
    "env": {
      "commonjs": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 100
        }
      ]
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-fp-ts": "^0.3.2",
    "eslint-plugin-functional": "^4.4.1",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.8.3"
  },
  "peerDependencies": {
    "eslint": "^8.25.0"
  },
  "devDependencies": {
    "eslint": "^8.32.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "pnpm": "^7.25.0",
    "semantic-release": "^20.0.2",
    "typescript-language-server": "^2.3.0"
  }
}
